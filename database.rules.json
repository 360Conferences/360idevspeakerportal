{
  "rules": {
    "config": {
      ".read": true,
      ".write": false
    },
    "$year": {
      "profiles": {
        "$uid": {
          ".read": "auth.uid != null",
          ".write": "auth.uid == $uid"
        }
      },
      "submissions": {
        ".read": "auth.uid != null",
        ".write": "auth.uid != null"
      },
      "sessions": {
        ".read": true,
        ".write": false
      },
      "speakers": {
        ".read": true,
        ".write": false
      },
      "venue": {
         ".read": true,
        ".write": false
      },
      "feedback": {
        ".read": true,
        ".write": false
      }
    }
  }
}
