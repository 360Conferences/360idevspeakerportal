{
  "rules": {
    "2016": {
      "sessions": {
        ".read": true,
        ".write": false
      },
      "speakers": {
        ".read": true,
        ".write": false
      },
      "venue": {
        ".read": true,
        ".write": false
      },
      "feedback": {
        ".read": true,
        ".write": true
      }
    },
    "2017": {
      "profiles": {
        "$uid": {
          ".read": true,
          ".write": "auth.uid == $uid"
        }
      },
      "submissions": {
        ".read": true,
        ".write": "auth.uid != null"
      }
    }
  }
}
