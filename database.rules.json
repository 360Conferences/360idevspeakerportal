{
  "rules": {
    "config": {
      ".read": true,
      ".write": "root.child('config/admins').hasChild(auth.uid)"
    },
    "events": {
      "$eventid": {
        "profiles": {
          "$uid": {
            ".read": "auth.uid == $uid",
            ".write": "auth.uid == $uid"
          },
          ".read": "root.child('config/admins').hasChild(auth.uid)",
          ".write": "root.child('config/admins').hasChild(auth.uid)"
        },
        "submissions": {
          ".read": "auth.uid != null",
          ".write": "auth.uid != null"
        },
        "schedule": {
          ".read": true,
          ".write": "root.child('config/admins').hasChild(auth.uid)"
        },
        "venue": {
           ".read": true,
          ".write": "root.child('config/admins').hasChild(auth.uid)"
        },
        "feedback": {
          ".read": true,
          ".write": false
        }
      }
    }
  }
}
