{
  "rules": {
    "events": {
      "$eventid": {
        "config": {
          ".read": true,
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "profiles": {
          "$uid": {
            ".read": true,
            ".write": "auth.uid == $uid"
          },
          ".read": true,
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "submissions": {
          ".read": true,
          ".write": "auth.uid != null"
        },
        "schedule": {
          ".read": true,
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "venue": {
           ".read": true,
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "feedback": {
          ".read": true,
          ".write": false
        }
      },
      ".read": true,
      ".write": false
    }
  }
}
